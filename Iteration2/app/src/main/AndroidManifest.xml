<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.iteration2.mark.iteration2" >

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WAKE_LOCK" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme"        >
        <activity
            android:name=".MainJava"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".SecondSlide"
            android:label="@string/app_name"
            android:parentActivityName=".MainJava" >
            <!--<meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.videotest.mark.videotest.MainActivity" />-->
        </activity>
        <activity
            android:name=".ThirdSlide"
            android:label="@string/app_name"
            android:parentActivityName=".SecondSlide" >
            <!--<meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.videotest.mark.videotest.MainActivity" />-->
        </activity>
    </application>

</manifest>
